    "node-nats-streaming": "^0.3.2",
    "nodemon": "^2.0.18",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6"
